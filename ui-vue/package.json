{
  "name": "fast-json-ui-vue",
  "version": "1.0.0",
  "description": "A Vue 3 library for converting JSON to UI components",
  "main": "dist/fast-json-ui-vue.umd.js",
  "module": "dist/fast-json-ui-vue.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "build:watch": "vite build --watch",
    "build:demo": "vite build && vue-tsc --emitDeclarationOnly && echo '构建完成，demo将自动更新'",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "json",
    "ui",
    "components",
    "fast-json-ui"
  ],
  "author": "Fast-JSON-UI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlwaysSum/fast-json-ui.git"
  },
  "homepage": "https://github.com/AlwaysSum/fast-json-ui",
  "bugs": {
    "url": "https://github.com/AlwaysSum/fast-json-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  }
}
